<% local_params = { from: params[:from], to: params[:to]} %>
<%if params[:account] %>
  <% local_params = local_params.merge({account: params[:account]})%>
<%end%>
<%if params[:transactions_type] %>
  <% local_params = local_params.merge({transactions_type: params[:transactions_type]})%>
<%end%>
<%= render :template => 'vouchers/index', locals: { params: local_params, :@vouchers =>  get_by_date(session[:previous_tab], params[:from], params[:to], params[:account], params[:transactions_type]).page(params[:page])} %>
