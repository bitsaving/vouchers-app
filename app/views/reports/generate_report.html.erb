<%= @voucher_startDate %>
<%local_params = {from: @voucher_startDate.to_date, to: @voucher_endDate.to_date } %>
<%if @voucher_accountName.present? %>
  <%local_params = local_params.merge({report_account: @voucher_accountName})%>
<%end%>
<%if @voucher_accountType.present? %>
  <%local_params = local_params.merge({account_type: @voucher_accountType})%>
<%end%>
<%= render :template => 'vouchers/index', locals: {params: local_params, :@vouchers =>  get_by_date(session[:previous_tab], @voucher_startDate,@voucher_endDate, @voucher_accountName, @voucher_accountType).page(params[:page])} %>
