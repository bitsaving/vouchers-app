<div class="account_name">
  <strong>Vouchers of account :</strong> <%= @account.name %>
</div>
<%= select_tag :account, options_for_select(ACCOUNT_FILTER_OPTIONS, :selected => params[:transaction_type]), data:{ path: account_vouchers_path(@account.id), name: @account.id }  %>
 <div class="alignment pull-right">
  <%= link_to "Back", "", :class => 'btn btn-primary', :onclick =>' history.go(-1); return false' %>
  <%= link_to "Edit", edit_account_path(@account), :class => 'btn btn-primary' %>
</div>
