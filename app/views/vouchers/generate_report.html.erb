<%local_params = {from: @voucher_startDate, to: @voucher_endDate } %>
<%if @voucher_accountName.present? %>
  <%local_params = local_params.merge({report_account: @voucher_accountName})%>
<%end%>
<%if @voucher_accountType.present? %>
  <%local_params = local_params.merge({account_type: @voucher_accountType})%>
<%end%>
<%= render :template => 'vouchers/index' ,locals: {params: local_params ,:@vouchers =>  get_new_by_date(@voucher_startDate,@voucher_endDate,@voucher_accountName,@voucher_accountType).order('updated_at desc').page(params[:page]).per(10)}%>
